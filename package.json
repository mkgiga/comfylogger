{
  "name": "comfylogger",
  "version": "1.0.5",
  "description": "A simple and versatile logging library for both Node.js and browser environments.",
  "keywords": [
    "logging",
    "logger",
    "ansi",
    "terminal",
    "javascript",
    "nodejs",
    "typescript",
    "browser"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "./dist/logger.js",
  "types": "./dist/logger.d.ts",
  "exports": {
    ".": {
      "types": "dist/logger.d.ts",
      "import": "dist/logger.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "cleanup": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "prepublish": "npm run cleanup && npm run build",
    "test": "npm run prepublish && node dist/test/test.js"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0"
  }
}
